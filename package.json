{
  "private": true,
  "packageManager": "pnpm@7.13.0",
  "workspaces": [
    "packages/*",
    "examples/*",
    "docs"
  ],
  "scripts": {
    "dev": "pnpm -F w-design-vue dev",
    "build": "pnpm -F w-design-vue build",
    "web:dev": "pnpm -F w-design-vue-example dev",
    "play:dev": "pnpm -F playground dev",
    "docs:dev": "pnpm -C docs dev",
    "docs:build": "pnpm -C docs build",
    "docs:serve": "pnpm -C docs serve"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "esbuild": "^0.15.13",
    "prettier": "^2.7.1",
    "rollup": "^3.2.5",
    "typescript": "^4.8.4",
    "unplugin-vue-macros": "^1.1.1",
    "vite": "^3.2.2",
    "vite-plugin-dts": "^1.7.0",
    "vue": "^3.2.41"
  },
  "engines": {
    "node": "^14.18.0 || >=16"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite"
      ]
    }
  }
}