{
  "private": true,
  "packageManager": "pnpm@7.13.0",
  "workspaces": [
    "packages/*",
    "examples/*",
    "docs"
  ],
  "scripts": {
    "dev": "pnpm -F w-design-vue dev",
    "build": "pnpm -F w-design-vue build",
    "web:dev": "pnpm -F w-design-vue-example dev",
    "play:dev": "pnpm -F playground dev",
    "icons:build": "pnpm -F icons build",
    "docs:dev": "pnpm -C docs dev",
    "docs:build": "pnpm -C docs build",
    "docs:serve": "pnpm -C docs serve"
  },
  "devDependencies": {
    "esbuild": "^0.18.16",
    "eslint": "^8.48.0",
    "prettier": "^3.0.2",
    "rollup": "^3.26.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.6"
  },
  "engines": {
    "node": "^14.18.0 || >=16.x",
    "pnpm": "^7.13.0 || >=8.x"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite"
      ]
    }
  }
}