{
  "private": true,
  "packageManager": "pnpm@7.13.0",
  "workspaces": [
    "packages/*",
    "examples/*",
    "docs"
  ],
  "scripts": {
    "dev": "pnpm -F w-design-vue dev",
    "build": "pnpm -F w-design-vue build",
    "web:dev": "pnpm -F w-design-vue-example dev",
    "play:dev": "pnpm -F playground dev",
    "docs:dev": "pnpm -C docs dev",
    "docs:build": "pnpm -C docs build",
    "docs:serve": "pnpm -C docs serve"
  },
  "devDependencies": {
    "esbuild": "^0.18.2",
    "rollup": "^3.25.1",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  },
  "engines": {
    "node": "^14.18.0 || >=16"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite"
      ]
    }
  }
}