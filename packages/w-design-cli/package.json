  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/node": "^20.11.5",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "esbuild": "^0.18.16",
    "execa": "^7.1.1",
    "minimist": "^1.2.8",
    "rollup": "^3.26.3",
