{
  "name": "w-design-vue",
  "version": "1.0.0",
  "title": "W Design Vue",
  "description": "A simple components library based on Vue 3.x",
  "type": "module",
  "packageManager": "pnpm@7.13.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/w-design-vue.min.js",
  "browser": "dist/w-design-vue.min.js",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./lib/index.js"
    },
    "./resolver": {
      "import": "./es/resolver/index.js",
      "require": "./lib/resolver/index.js"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "lib",
    "es",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:package": "rollup -c",
    "preview": "vite preview"
  },
  "keywords": [
    "ui",
    "vue",
    "vue3",
    "components",
    "component library",
    "ui framework",
    "w-design-vue"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weijuer/w-design-vue.git"
  },
  "author": "Weijuer<https://github.com/weijuer>",
  "license": "MIT",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/w-design-vue"
  },
  "homepage": "https://github.com/weijuer/w-design-vue/tree/master/#readme",
  "bugs": "https://github.com/weijuer/w-design-vue/issues",
  "peerDependencies": {
    "vue": ">=3.2.0"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "dependencies": {
    "@w-design/eslint-config": "workspace:^",
    "@w-design/icons": "workspace:^",
    "@w-design/use": "workspace:^",
    "@w-design/utils": "workspace:^"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@vitejs/plugin-vue": "^5.0.4",
    "deepmerge": "^4.3.1",
    "esbuild": "^0.20.2",
    "fast-glob": "^3.3.2",
    "rollup": "^4.14.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.8.1",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.13"
  },
  "browserslist": [
    "> 1%",
    "not ie 10",
    "not op_mini all"
  ]
}