{
  "name": "@w-design/w-design-vue",
  "version": "1.0.0",
  "title": "W Design Vue",
  "description": "A simple components library based on Vue 3.x",
  "type": "module",
  "packageManager": "pnpm@7.13.0",
  "main": "lib/w-design-vue.umd.cjs",
  "module": "lib/w-design-vue.js",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/w-design-vue.js",
      "require": "./lib/w-design-vue.umd.cjs"
    },
    "./lib/index.css": "./lib/style.css",
    "./package.json": "./package.json"
  },
  "files": [
    "es",
    "lib",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "keywords": [
    "ui",
    "vue",
    "vue3",
    "components",
    "component library",
    "ui framework",
    "w-design-vue"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weijuer/w-design-vue.git"
  },
  "author": "Weijuer<https://github.com/weijuer>",
  "license": "MIT",
  "homepage": "https://github.com/weijuer/w-design-vue/tree/master/#readme",
  "bugs": "https://github.com/weijuer/w-design-vue/issues",
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@w-design/w-design-icon": "workspace:*"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "typescript": "^5.0.4",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.1",
    "vue-tsc": "^1.4.4",
    "vue": "^3.2.47"
  },
  "engines": {
    "node": "^14.18.0 || >=16"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite"
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "not ie 10",
    "not op_mini all"
  ]
}